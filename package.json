    "uglifyjs-webpack-plugin": "2.1.2",
    "vue-jest": "3.0.4",
    "vue-loader": "15.7.0",
    "vue-svg-inline-loader": "1.2.15",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.1",
    "webpack-dev-server": "3.3.1",
    "webpack-merge": "4.2.1"
