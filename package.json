    "@babel/preset-env": "7.4.4",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios-mock-adapter": "1.16.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.7.1",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "2.0.1",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
